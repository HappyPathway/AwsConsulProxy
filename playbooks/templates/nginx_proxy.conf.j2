upstream backend {
  <% with key "services/{{ env }}/{{ service_name }}/version" %>
  <% range service ( printf "%s.{{ app_name }}" . ) %>
  server <% .Address %>:<% key "services/{{ env }}/{{ service_name }}/port" %>;
  <% end %>
  <% end %>
}

server {
    listen {{ service_port }};
    server_name {{ proxy_name }};
    location / {
      proxy_pass http://backend;
    }
}

